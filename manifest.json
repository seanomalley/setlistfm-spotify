{
  "manifest_version": 2,
  "name": "Setlistify",
  "version": "1.0",
  "description": "Makes Spotify playlists from setlist.fm pages",
  "browser_action": {
    "default_icon": "favicon.png",
	"default_popup": "popup.html"
  },
  "permissions": [
    "http://ws.spotify.com/",
    "http://www.setlist.fm/"
  ],
  "content_scripts": [
  	{
	  	"matches": ["http://www.setlist.fm/*"],
	  	"js": ["jquery-1.6.2.js", "content.js"]
  	}
  ]
}