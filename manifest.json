{
  "name": "Setlistify",
  "version": "1.0",
  "description": "Integrates setlist.fm with spotify",
  "browser_action": {
    "default_icon": "favicon.ico"
  },
  "permissions": [
    "http://ws.spotify.com/",
    "http://www.setlist.fm/"
  ],
  "background_page": "background.html",
  "content_scripts": [
  	{
	  	"matches": ["http://www.setlist.fm/*"],
	  	"js": ["jquery-1.6.2.js", "content.js"]
  	}
  ]
}